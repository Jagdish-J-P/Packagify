{
    "name": "jagdish-j-p/packagify",
    "description": "Package to create empty package directory structure in project.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jagdish-J-P",
            "email": "jagdish1230@gmail.com"
        }
